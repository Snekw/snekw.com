<div class="container">
  <p>Attached uploads</p>
  <div id="attached-uploads-target">
    {{#each article.uploads}}
      {{>uploadBrowserTemplate attached=true}}
    {{/each}}
  </div>
</div>
<div class="container">
  <p>Available uploads</p>
  <div id="available-uploads-target">
    {{#each availableUploads}}
      {{>uploadBrowserTemplate}}
    {{/each}}
  </div>
</div>
<template id="upload-template">
  {{>uploadBrowserTemplate}}
</template>
<script src="/static/js/request.js"></script>
<script src="/static/js/uploadBrowser.js"></script>