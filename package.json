{
  "name": "snekw.com",
  "version": "0.2.0",
  "repository": "https://github.com/Snekw/snekw.com",
  "license": "GPL-3.0",
  "scripts": {
    "bsync": "browser-sync start --proxy \"dev.local:3000\" --files \"src/static/css/*.css\"",
    "scss": "scss src/scss/main.scss src/static/css/main.css --style expanded",
    "scss:w": "scss --watch src/scss:src/static/css --style expanded",
    "build": "node tasks/build.js",
    "copyDevConfig": "node tasks/copyDevConfigToDist.js",
    "test": "cross-env NODE_ENV=test mocha --recursive ./test/spec --exit",
    "check-coverage": "nyc --reporter=html --all npm test",
    "coverage": "nyc report --reporter=text-lcov"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "coverage/**",
      "test/**",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "src/config/**",
      "src/config-example/**",
      "src/static/**/third-party/**"
    ]
  },
  "dependencies": {
    "auth0-js": "^8.8.0",
    "autoprefixer": "^7.2.2",
    "body-parser": "^1.18.2",
    "clean-css": "^4.1.9",
    "commonmark": "^0.28.1",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.3.2",
    "critical": "^0.9.1",
    "css": "^2.2.1",
    "csurf": "^1.9.0",
    "debug": "^2.6.9",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fs-extra": "^4.0.3",
    "handlebars": "^4.0.11",
    "hbs": "^4.0.1",
    "helmet": "^3.9.0",
    "moment": "^2.19.4",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "passport": "^0.4.0",
    "passport-auth0": "^0.6.0",
    "postcss": "^6.0.14",
    "prismjs": "^1.9.0",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "rotating-file-stream": "^1.3.4",
    "serve-favicon": "^2.4.5",
    "shortid": "^2.2.8",
    "uglify-js": "^3.2.2",
    "validator": "^8.2.0"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "proxyquire": "^1.8.0",
    "sinon": "^4.1.3"
  }
}
